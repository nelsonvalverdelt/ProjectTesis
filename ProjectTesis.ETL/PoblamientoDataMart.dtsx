                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
Orbitum_Mart.dbo.Dim_Cliente.Cliente_Key,
Orbitum_Mart.dbo.Dim_Empleado.Empleado_Key,
Orbitum_Mart.dbo.Dim_Tiempo.Tiempo_Key,
Orbitum_Mart.dbo.Dim_Producto.Producto_Key,
PrecioUnitario as Precio,
Cantidad,
(OrdenDetalle.PrecioUnitario * OrdenDetalle.Cantidad) as Monto,
Orden.Recargo as RecargoUsoTarjeta,
((OrdenDetalle.PrecioUnitario * OrdenDetalle.Cantidad) + ((OrdenDetalle.PrecioUnitario * OrdenDetalle.Cantidad) *Recargo)) as MontoTotal
FROM Orden
INNER JOIN OrdenDetalle ON Orden.Id = OrdenDetalle.OrdenId
INNER JOIN Orbitum_Mart.dbo.Dim_Cliente ON Orbitum_Mart.dbo.Dim_Cliente.ClienteId = Orden.ClienteId
INNER JOIN Orbitum_Mart.dbo.Dim_Empleado ON Orbitum_Mart.dbo.Dim_Empleado.EmpleadoId = Orden.EmpleadoId
INNER JOIN Orbitum_Mart.dbo.Dim_Tiempo ON Orbitum_Mart.dbo.Dim_Tiempo.Fecha = Orden.FechaEmision
INNER JOIN Orbitum_Mart.dbo.Dim_Producto ON Orbitum_Mart.dbo.Dim_Producto.ProductoId = OrdenDetalle.ProductoId
WHERE Estado = 'DESPACHADO COBRADO'</property>
